<%- model_class = Tweet -%>
<div class = "row-fluid">
<div class = "span3">
<%= image_tag @user.photo.url(:small) %>
</div>
<%= render :partial => 'form' %>
</div>
<div class ="row-fluid">
<div class = "span3" style="padding-top:50px">
<p>
            <%= link_to "Following", user_following_path(current_user) %>
</p>
<p>
<%= link_to "Followers", user_followers_path(current_user) %>
</p>
</div>
<div class = "span9 " id="feed">

<%= render :partial => 'dashboard' %>
</div>
</div>

